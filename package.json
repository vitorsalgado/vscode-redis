        "category": "Redis"
      },
      {
        "command": "extension.redis.end",
        "title": "Close | End",
        "category": "Redis"
      },
      {
        "command": "extension.redis.llen",
        "title": "LLEN",
        "category": "Redis"
      },
      {
        "command": "extension.redis.lpush",
        "title": "LPUSH",
        "category": "Redis"
      },
      {
        "command": "extension.redis.rpush",
        "title": "RPUSH",
        "category": "Redis"
      },
      {
        "command": "extension.redis.lpushx",
